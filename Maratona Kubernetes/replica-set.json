{
    "apiVersion": "extensions/v1beta1",
    "kind": "ReplicaSet",
    "metadata": {
        "name": "estudo-rc"
    },
    "spec": {
        "replicas": 5,
        "selector": {
            "matchLabels": {
                "version": "v1",
                "app": "api-estudo"
            }
        }
    },
    "template": {
        "metadata": {
            "labels": {
                "version": "v1",
                "app": "api-estudo"
            }
        },
        "spec": {
            "containers": [
                {
                    "name": "api-estudo",
                    "image": "nginx:1.17-alpine",
                    "ports": [{"containerPort": 4000}]
                }
            ]
        }
    }
}